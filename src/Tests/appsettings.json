{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Rebus": {
    "DefaultBus": "Default",
    "Buses": {
      "Default": {
        "Transport": {
          "QueueName": "manager",
          "ProviderName": "FileSystem",
          "Providers": {
            "FileSystem": {
              "BaseDirectory": "{PWD}/transport/"
            }
          }
        }
      }
    }
  }
}
